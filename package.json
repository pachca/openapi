{
  "name": "api-docs",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.3.4",
  "scripts": {
    "predev": "npm run generate:openapi",
    "dev": "next dev --turbopack -H 0.0.0.0",
    "generate:openapi": "tsp compile openapi-source/typespec.tsp --config openapi-source/tspconfig.yaml",
    "prebuild": "npm run generate:openapi && node scripts/check-url-conflicts.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-urls": "node scripts/check-url-conflicts.mjs"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@next/mdx": "^15.1.6",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@types/flexsearch": "^0.7.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.6",
    "flexsearch": "^0.7.43",
    "gray-matter": "^4.0.3",
    "gsap": "^3.13.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.469.0",
    "next": "16.0.10",
    "next-mdx-remote": "^5.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rehype-highlight": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "shiki": "^1.23.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/react-dom": "^19.2.3",
    "@typespec/compiler": "^1.7.1",
    "@typespec/http": "^1.7.0",
    "@typespec/openapi": "^1.7.0",
    "@typespec/openapi3": "^1.7.0",
    "@typespec/rest": "^0.77.0",
    "tailwindcss": "^4",
    "turbo": "^2.7.6"
  }
